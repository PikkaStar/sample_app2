
<h1>新規投稿</h1>
<% if @list.errors.any? %>
<%= @list.errors.count %>件のエラーが発生しました
<% @list.errors.full_messages.each do |message| %>
<li>
  <%= message %>
</li>
<% end %>
<% end %>
<%= form_with model: @list do |f| %>
<h4>タイトル</h4>
<%= f.text_field :title %>
<h4>投稿</h4>
<%= f.text_area :body %>
<%= f.file_field :image,accept: "image/*" %>
<%= f.submit "投稿" %>

<% end %>